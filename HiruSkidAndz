        pcall(function()
            if _G.Gan_Duoc_Cho_Khi and AnDepZai3 then
                if game.Players.LocalPlayer.Backpack:FindFirstChild("Holy Torch") or game.Players.LocalPlayer.Character:FindFirstChild("Holy Torch") then
