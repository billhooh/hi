    local ToggleSailBoat = Tabs.Sea:AddToggle("ToggleSailBoat", {Title = "Auto Buy Boat + Zone 6",Description = "", Default = false })
    ToggleSailBoat:OnChanged(function(Value)
        _G.SailBoat = Value
    end)
