                if game:GetService("Workspace"):FindFirstChild("AttachedAzureEmber") then
                    TP2(game:GetService("Workspace"):WaitForChild("EmberTemplate"):FindFirstChild("Part").CFrame)
					print("qua r ne")
                end
            end)
        end
    end
end)
