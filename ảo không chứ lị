    Title = "Auto Kill Ghost Ship",
    Description = "Enable for kill ghost ship if have",
    Default = _G.RelzFishBoat
})
ghostship:OnChanged(function(value)
    _G.RelzFishBoat = value
    saveSettings()
    if not _G.RelzFishBoat then
        _G.SeaSkill = false
        Skillaimbot = false
    end
end)
