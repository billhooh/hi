                    end
    end
})


local ToggleHumanandghoul = Tabs.Race:AddToggle("ToggleHumanandghoul", {Title = "Auto Trial Human Ghoul",Description = "", Default = false })
ToggleHumanandghoul:OnChanged(function(Value)
    KillAura = Value
